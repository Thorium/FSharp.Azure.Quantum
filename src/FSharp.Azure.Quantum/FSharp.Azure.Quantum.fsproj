<Project Sdk="Microsoft.NET.Sdk">

  <PropertyGroup>
    <TargetFramework>net10.0</TargetFramework>
    <GenerateDocumentationFile>true</GenerateDocumentationFile>
    <GenerateProgramFile>false</GenerateProgramFile>

    <RepositoryUrl>https://github.com/thorium/FSharp.Azure.Quantum</RepositoryUrl>
    <PackageProjectUrl>https://github.com/thorium/FSharp.Azure.Quantum</PackageProjectUrl>

    <Authors>Tuomas Hietanen</Authors>
    <PackageTags>F#;FSharp;Quantum;Azure;Computing</PackageTags>
    <PackageReadmeFile>README.md</PackageReadmeFile>
    <PackageLicenseExpression>Unlicense</PackageLicenseExpression>
    <SymbolPackageFormat>snupkg</SymbolPackageFormat>

  </PropertyGroup>

  <ItemGroup>
    <Compile Include="Types.fs" />
    <Compile Include="Cost.fs" />
    <Compile Include="CostEstimation.fs" />
    <Compile Include="Authentication.fs" />
    <Compile Include="Retry.fs" />
    <Compile Include="Client.fs" />
    <Compile Include="TspSolver.fs" />
    <Compile Include="TspBuilder.fs" />
    <Compile Include="PortfolioSolver.fs" />
    <Compile Include="ProblemAnalysis.fs" />
    <Compile Include="QuantumAdvisor.fs" />
    <Compile Include="CircuitBuilder.fs" />
    <Compile Include="QuboEncoding.fs" />
    <Compile Include="QaoaCircuit.fs" />
  </ItemGroup>

  <ItemGroup>
    <PackageReference Include="Azure.Identity" Version="1.13.1" />
    <PackageReference Include="MathNet.Numerics" Version="5.0.0" />
    <PackageReference Include="MathNet.Numerics.FSharp" Version="5.0.0" />
    <PackageReference Include="Microsoft.Extensions.Logging" Version="8.0.0" />
  </ItemGroup>

</Project>
